// @flow

interface GeldOptions {
	currency?: string;
	currencyPosition?: 'before' | 'after';
	decimals?: number;
	decimalSeparator?: string;
	orderSeparator?: string;
	zeroDecimals?: string;
	space?: string;
}

declare export default function geld(
	value: number | string,
	options?: GeldOptions
): string;
